game:GetService('StarterGui'):SetCore('SendNotification', {Title = 'By imie#0039', Text = 'https://iexploit.xyz\nhttps://iexploit.xyz/discord'})

local plr = game:GetService('Players').LocalPlayer
local character = plr.Character

plr.Chatted:Connect(function(msg)
if msg == '/e red' then

for _,v in pairs(plr:WaitForChild('Backpack'):GetChildren()) do
if v.Name == '[Knife]' then
    v.Handle.Material = 'ForceField'
    v.Handle.Color = Color3.fromRGB(255,0,0)
    end
end

for _,v in pairs(plr.Character:GetChildren()) do
if v.Name == '[Knife]' then
    v.Handle.Material = 'ForceField'
    v.Handle.Color = Color3.fromRGB(255,0,0)
    end
end
end

if msg == '/e blue' then

for _,v in pairs(plr:WaitForChild('Backpack'):GetChildren()) do
if v.Name == '[Knife]' then
    v.Handle.Material = 'ForceField'
    v.Handle.Color = Color3.fromRGB(0, 0, 255)
    end
end

for _,v in pairs(plr.Character:GetChildren()) do
if v.Name == '[Knife]' then
    v.Handle.Material = 'ForceField'
    v.Handle.Color = Color3.fromRGB(0, 0, 255)
    end
end
end
end)
